     name : OnReturnDatatransPayment
     internal_properties : 
     procedure_id : 2220122836612135211
     type_code : 12
     group : 3
     code : |1-
      // Summary: Manage the Lyra return
      // Syntax:
      //[ <Result> = ] OnDatatransPaymentReturn (<nMode> is int, <sInformation> is string, <sAction> is string)
      //
      // Parameters:
      //	nMode (integer): (output) Error mode
      //	sInformation (ANSI string): (output) Information about the return
      //	sAction (ANSI string): value among	URL_S2S, URL_CANCEL, URL_REFUSED, URL_OK
      // Return value:
      // 	boolean:             True if the return was managed
      //
      Procédure OnReturnDatatransPayment(nMode est un entier, sInformation est une chaine, sAction est une chaine)
      
      	// Reset errors
      	XERROR()
      
      	//S2S return?
      	SI sAction = URL_S2S ALORS
      		// Check the issuer IP
      		SI PAS BankServerCheckAddress(nMode, sInformation)	 ALORS
      			RENVOYER Faux
      		FIN
      	FIN
      
      	////checks the signature
      	RetrieveParametersReceived()
      	
      	
      	
      	selon PageParamètre("status")
      		CAS "success"
      			
      			//build the parameters used for the signature
      			sSignature2 				est une chaine
      			sSignature2Build		est une chaine
      			//sign2=merchantId+amount+currency+uppTransactionId 					
      			sSignature2 = 	SiteIdentifier+ 
      			PartieEntière(m_cyAmount * 100) +
      			CurrencyToString()+
      			PageParamètre("uppTransactionId")
      			
      			sSignature2Build = Minuscule(EncodeMAC(sSignature2,Vrai,AlgorithmHash,Sign2))			
      			// Does it correspond?
      			SI Majuscule(PageParamètre("sign2")) <> Majuscule(sSignature2Build) ALORS
      				nMode = PAYMENT_FORGED
      				sInformation = "Signature not found or not recognized" + RC + "Signature data: " + sSignature2 + RC + ...
      				"Parameters: " + "sign" + RC + "Calculated signature: " + sSignature2Build + RC + "Signature received: " + PageParamètre("sign2")
      				XERROR(sInformation)
      				RENVOYER Faux
      			SINON
      				XTRACE("Signature successfully checked")
      			FIN
      			
      			nMode = PAYMENT_VALID
      			sInformation = ""
      			
      		CAS "error"
      			nMode = PAYMENT_REFUSED
      			sInformation	 = "Payment refused, error code "+ PageParamètre("acqErrorCode")+[", "]+PageParamètre("errorCode")+", "+PageParamètre("errorMessage")+", "+PageParamètre("errorDetail")
      								
      		cas "cancel"	
      			nMode = PAYMENT_CANCELED
      			sInformation	 = "Payment canceled"
      		AUTRE CAS
      			nMode = PAYMENT_REFUSED
      			sInformation	 = "Return with incorrect status control:  "+ PageParamètre("status") + rc +"error code: "+ PageParamètre("acqErrorCode")+[", "]+PageParamètre("errorCode")+", "+PageParamètre("errorMessage")+", "+PageParamètre("errorDetail")
      	FIN
      	
      XTRACE(sInformation)
      
      renvoyer vrai
