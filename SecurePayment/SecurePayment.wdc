      	// Concatenates all the lines
      	pour i = 1 _a_ MesParamètres..Occurrence
      		sError += [rc] + MesParamètres[i]
      	FIN
      		
      	// Keeps the error	
      	m_sErrorInfo += [rc] + sError
      	
