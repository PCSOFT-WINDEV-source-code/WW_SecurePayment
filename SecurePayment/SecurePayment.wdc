      //
      procedure global AliasWEBDEVEngine()
      
      sEngineAlias 	is string
      sWEBDEVVersion	is string
      	
      	// Retrieves the version of WEBDEV
      sWEBDEVVersion = NumToString(Val(WINDEVVersion), ".1f")
      	
