      		else
      			return True
      		end
      	else
      		Transaction.Parameters		=	""
      		Transaction.Information	=	""
      		Transaction.Loginfo			=	""
      		Transaction.PaymentObject	=	""
      		if not HModify(Transaction) then
      			XERROR(HErrorInfo())
      ErrorPropagate()
      			return False
      		else
      			return True
      		end
      	end		
      else
      	//not found therefore OK
      	return True
      end
